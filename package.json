{
  "name": "codex-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "debug.css": "codex-src/debug.css",
    "emoji-trie": "https://github.com/codex-src/emoji-trie.js",
    "material-colors.css": "codex-src/material-colors.css",
    "minireset.css": "^0.0.6",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-feather": "^2.0.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "stylex": "https://github.com/codex-src/stylex.js",
    "system-font-css": "^2.0.2",
    "use-graphql": "codex-src/use-graphql.js",
    "use-methods": "^0.5.1"
  },
  "scripts": {
    "lint": "yarn eslint --ignore-pattern=build --ignore-pattern=\"**/*.min.js\" .",
    "tailwind-dev": "yarn postcss src/stylesheets/tailwind.src.css --output src/stylesheets/tailwind.css",
    "tailwind-prod": "yarn postcss src/stylesheets/tailwind.src.css --output src/stylesheets/tailwind.css --env production",
    "start": "yarn tailwind-dev && react-scripts start",
    "start-build": "yarn build && PORT=3000 serve --single --no-clipboard build",
    "test": "react-scripts test",
    "test-editor-chrome": "BROWSER=Chrome yarn test --verbose --watchAll=false --runInBand Editor/__tests/",
    "test-editor-firefox": "BROWSER=Firefox yarn test --verbose --watchAll=false --runInBand Editor/__tests/",
    "test-editor-safari": "BROWSER=Safari yarn test --verbose --watchAll=false --runInBand Editor/__tests/",
    "test-editor": "yarn test-editor-chrome && yarn test-editor-firefox && yarn test-editor-safari",
    "build": "yarn tailwind-prod && react-scripts build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@fullhuman/postcss-purgecss": "^2.0.6",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.7.2",
    "playwright": "^0.10.0",
    "postcss-cli": "^7.1.0",
    "serve": "^11.2.0",
    "tailwindcss": "^1.2.0"
  }
}
